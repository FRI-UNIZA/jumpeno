@namespace Jumpeno.Client.Components

@inherits TextComponent

<TextElement
    id="@ID"
    Type="TEXT_TYPE.ANCHOR"
    Variant="Variant"
    Size="Size"
    NoWrap="NoWrap"
    Spacing="false"
    Class="@ComputeLinkClass()"
    Style="@Style"
    href="@Href"
    target="@ComputeTarget()"
    tabindex="@TabIndex"
    role="@ROLE"
    @onclick="OnClickEvent"
    @onkeypress="OnEnterEvent"
    @attributes="@AdditionalAttributes"
>
    @ChildContent
</TextElement>
